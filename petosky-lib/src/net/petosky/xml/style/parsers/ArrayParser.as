package net.petosky.xml.style.parsers {	/**	 * @author cpetosky	 */	final public class ArrayParser {		public static function parseFloats(s:String):Array {			var vals:Array = s.split(" ");			for (var i:uint = 0; i < vals.length; ++i)				vals[i] = parseFloat(vals[i]);			return vals;		}				public static function createSplitter(d:String):Function {			return function(s:String):Array {				return s.split(d);			};		}	}}