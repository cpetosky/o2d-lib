package net.petosky.collections {	/**	 * Any object that stores multiple pieces of data should implement ICollection.	 * @author cpetosky	 */	public interface ICollection {		/**		 * @return the number of elements in this Collection.		 */		function get length():uint;				/**		 * @return true if and only if length == 0.		 */		function get empty():Boolean;				/**		 * Add an element to a Collection.		 * @param args the element(s) to add		 * @return true if additions were successful.		 */		function add(...args):Boolean;				/**		 * Add an array of elements to the Collection		 * @param a Array of elements to add		 * @return true if additions were successful		 */		function addArray(a:Array):Boolean;				/**		 * @return a textual representation of this Collection.		 */		function toString():String;				/**		 * @return an array containing all elements of this Collection.		 */		function toArray():Array;				/**		 * Remove all elements from this container.		 */		function clear():void;	}}